@page
@model Zealand_semester1_eksamen.Pages.Boats.CreateBoatSJSModel
@{
    <form method="post">
        <div class="form-group">
            <label asp-for="boat.Name" class="control-label"></label>
            <input asp-for="boat.Name" class="form-control" />
            <span asp-validation-for="boat.Name" class=" text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="boat.SailNumber" class="control-label"></label>
            <input asp-for="boat.SailNumber" class="form-control" />
            <span asp-validation-for="boat.SailNumber" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="boat.Dimensions" class="control-label"></label>
            <input asp-for="boat.Dimensions" class="form-control" />
            <span asp-validation-for="boat.Dimensions" class="text-danger"></span>
        </div>

        <p>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </p>
    </form>
}
